export IDA71='/Users/troy/installers/71'
export DTXMSG='/Users/troy/installers/71/sdk/plugins/dtxmsg'

alias cd71='cd $IDA71'
alias cdtx='cd $DTXMSG'

alias dtxmsg_client='$IDA71/sdk/bin/dtxmsg_client'

